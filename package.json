{
  "name": "codeclimate-eclint",
  "version": "0.0.1",
  "description": "eclint",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "eclint": "^2.0.1",
    "glob": "^7.1.1",
    "isbinaryfile": "^3.0.2",
